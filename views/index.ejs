<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Datos de Cliente</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
</head>
<body>
    <header>
      <nav class="navbar navbar-expand-lg bg-body-tertiary">
          <div class="container-fluid">
            <a class="navbar-brand" href="/ver_servicios">Servicios</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
              <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
              <div class="navbar-nav">
                <a class="nav-link active" aria-current="page" href="/">Clientes</a>
                <a class="nav-link" href="/ver_horas">Ver Horas</a>
                <a class="nav-link" href="/ver_empleados">Empleados</a>
                <a class="nav-link" href="/ver_productos">Productos</a>
                <a class="nav-link" href="/venta">Venta</a>
                <a class="nav-link" href="/pedidos">Pedidos</a>
                <a class="nav-link" href="/ver_proveedores">Proveedores</a>
                <a class="nav-link" href="/analisis" data-bs-toggle="modal" data-bs-target="#exampleModal">Ver informe</a>
              </div>
            </div>
          </div>
        </nav>
    </header>  
      <div class="container d-flex text-center col-6 justify-content-center" style="padding-top: 2%; padding-bottom: 2%;">
        <table class="table border border-primary">
            <thead>
              <tr>
                <th scope="col">ID</th>
                <th scope="col">RUT DEL EMPLEADO</th>
            
                <th scope="col">NOMBRE EMPLEADO</th>
                <th scope="col">TIPO EMPLEADO</th>
              </tr>
            </thead>
            <tbody>
                <% users.forEach(user => { %>
                    <tr>
                        <th scope="row"><%= user.AIDI %></th>
                        <td><%= user.RUTER %></td>
                        <td><%= user.NEIM %></td>
                        <td><%= user.TIPO %></td>
                    </tr>  <!-- Modify this based on your database schema -->
                <% }); %>
            </tbody>
        </table>
    </div> 
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
</body>

</html>