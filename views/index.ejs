<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/CSS/inicio.css">
    <title>Datos de Cliente</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
</head>

<body onload="Instancia()">
    <!--ID DE LA BARRA DE NAVEGACIÓN-->
    <div id="navbar-container"></div>

    <div class="container d-flex text-center col-6 justify-content-center" style="padding-top: 2%; padding-bottom: 2%;">
        <table class="table border border-primary">
            <thead>
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">RUT DEL EMPLEADO</th>

                    <th scope="col">NOMBRE EMPLEADO</th>
                    <th scope="col">TIPO EMPLEADO</th>
                </tr>
            </thead>
            <tbody>
                <% users.forEach(user=> { %>
                    <tr>
                        <th scope="row">
                            <%= user.AIDI %>
                        </th>
                        <td>
                            <%= user.RUTER %>
                        </td>
                        <td>
                            <%= user.NEIM %>
                        </td>
                        <td>
                            <%= user.TIPO %>
                        </td>
                    </tr>
                    <% }); %>
            </tbody>
        </table>
    </div>
    <div id="footer-container"></div>
    <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
    <script src="/Js/agregarCarrito.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
        crossorigin="anonymous"></script>
    <!--SCRIPT DE LA BARRA DE NAVEGACIÓN-->
    <script src="/Js/fetchNav.js"></script>
    <!--FIN SCRIPT DE LA BARRA DE NAVEGACIÓN-->
</body>

</html>